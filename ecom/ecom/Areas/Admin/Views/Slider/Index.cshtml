@model List<Slider>
<div class="page-content">
    <div class="container-fluid">

      


        <div class="row">
            <div class="col-lg-12">
                <div class="card job-filter">
                    <div class="card-body p-3">
                        <table class="table">
                            <thead>
                                <tr>
                                    <th scope="col">Sub Title</th>
                                    <th scope="col">Title</th>
                                    <th scope="col">Price</th>
                                    <th scope="col">Image</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@item.SubTitle</td>
                                        <td>@item.Title</td>
                                        <td>@item.Price</td>
                                        <td> <img width="200" height="200" src="~/demo/img/slider/@item.ImgUrl" /></td>
                                    </tr>
                                };

                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
            <!--end col-->
        </div>
        <!--end row-->


    </div> <!-- container-fluid -->
</div><!-- End Page-content -->
